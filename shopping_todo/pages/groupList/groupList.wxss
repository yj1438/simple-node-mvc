.spider image {
    width: 290rpx;
    height: 410rpx;
    position: absolute;
    top: 0;
    right: 0;
}

.title-tips {
    margin-top: 336rpx;
    text-align: center;
}
.title-tips .l1 {
    line-height: 1;
    font-size: 40rpx;
    color: #000;
    margin-bottom: 30rpx;
}
.title-tips .l2 {
    font-size: 28rpx;
    color: #888888;
    line-height: 40rpx;
}
.btn-area {
    margin-top: 60rpx;
}
.btn-area .button {
    width: 670rpx;
    height: 94rpx;
    color: #fff;
    border: 1px solid #4169e1;
    background-color: #4169e1;
    font-size: 36rpx;
}

.group-area {
    box-sizing: border-box;
    width: 100%;
    padding: 40rpx 32rpx;
}

.icon-state {
    width: 28rpx;
    height: 28rpx;
    background-image: url(data:image/png;base64,);
    background-size: contain;
    margin-top: 10rpx;
}

.has .icon-state {
    background-image: url(data:image/png;base64,);
}

.group-item {
    margin-top: 28rpx;
    display: flex;
    justify-content: space-between;
}

.group-item .info {
    width: 628rpx;
    flex-shrink: 0;
    display: flex;
    justify-content: space-between;
    line-height: 1;
    padding-bottom: 28rpx;
    border-bottom: 1px solid #EEEEEE;
}

.info .l1{
    color: #999999;
    font-size: 32rpx;
    font-weight: bold;
    line-height: 46rpx;
}
.info .l2 {
    color: #999999;
    font-size: 28rpx;
    margin-top: 6rpx;
    line-height: 40rpx;
}
.info .l3 {
    color: #cccccc;
    font-size: 28rpx;
    margin-top: 6rpx;
    line-height: 40rpx;
}

.memCount {
    line-height: 40rpx;
    font-size: 28rpx;
    color: #999;
}

.has .l1{
    color: #4169e1;
}