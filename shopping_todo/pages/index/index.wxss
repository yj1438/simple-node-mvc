/**index.wxss**/
page {
	font-size: 16px;
	color: #333;
}
.join-members {
	box-sizing: border-box;
	padding: 32rpx;
	background-color: #F3F3F3;
	width: 100%;
}
.join-members .member{
	display: flex;
	justify-content: flex-start;
	flex-wrap: wrap;
	/*height: 192rpx;*/
}
.join-members .avatar-1 {
	margin: 0 12rpx 12rpx 0;
	flex-shrink: 0;
}
.avatar-1 {
	display: inline-block;
	width: 84rpx;
	height: 84rpx;
	overflow: hidden;
}
.avatar-2 {
	display: inline-block;
	width: 54rpx;
	height: 54rpx;
	overflow: hidden;
}
.join-members .title {
	color: #999999;
	font-size: 26rpx;
	line-height: 36rpx;
	height: 36rpx;
	display: block;
	margin-bottom: 8rpx;
}
.group-info {
	font-size: 48rpx;
	line-height: 1;
	margin-left: 32rpx;
	padding: 24rpx 32rpx 24rpx 0;
	color: #484D51;
	box-sizing: border-box;
	width: 718rpx;
	font-weight: bold;
	border-bottom: 1px solid #EEEEEE;
}
.group-info .btn-list {
	font-size: 32rpx;
	color: #4169e1;
	line-height: 48rpx;
	float: right;
	font-weight: normal;
}
.group-info input {
	display: inline-block;
}
.needs {
	margin-left: 32rpx;
	width: 718rpx;
	color: #484D51;
	font-size: 34rpx;
	line-height: 1;
	margin-bottom: 200rpx;
}
.need-item {
	box-sizing: border-box;
	width: 100%;
	display: flex;
	justify-content: space-between;
	/*align-items: center;
	align-self: center;*/
	position: relative;
}
.need-item .mask {
	width: 100%;
	height: 100%;
	position: absolute;
	left: 0;
	top: 0;
	background-color: rgba(255,255,255,.6);
}
.needs .state {
	display: inline-block;
	width: 40rpx;
	height: 40rpx;
	box-sizing: border-box;
	border: 1px solid #CCCCCC;
	border-radius: 50%;
	background-size: contain;
	margin-top:36rpx;
}
.state.doing {
	border: none;
	background-image: url(data:image/png;base64,);
}
.state.done {
	border: none;
	background-image: url(data:image/png;base64,);
}

.state.mine {
	border: none;
	background-image: url(data:image/png;base64,);
}
.needs .detail {
	width: 658rpx;
	box-sizing: border-box;
	padding: 28rpx 32rpx 28rpx 0;
	border-bottom: 1px solid #EEEEEE;
	display: flex;
	justify-content: space-between;
}
.needs .detail .cont {
	line-height: 54rpx;
	vertical-align: top;
	display:flex;
	width:480rpx;
	justify-content:flex-start;
}
.cont image {
	flex-shrink: 0;
	margin-right: 12rpx;
}
.cont text {
}

.operate .todo {
	display: inline-block;
	width: 120rpx;
	height: 60rpx;
	box-sizing: border-box;
	border: 1px solid #4169e1;
	border-radius: 10rpx;
	color: #4169e1;
	font-size: 26rpx;
	line-height: 60rpx;
	text-align: center;
}
.operate .doing, .operate .done {
	font-size: 26rpx;
	width: 120rpx;
	text-align: center;
	color: #999999;
	display: inline-block;
	line-height: 54rpx;
}
.operate .mine {
	display: inline-block;
	width: 120rpx;
	height: 60rpx;
	box-sizing: border-box;
	border: 1px solid #4169e1;
	background-color: #4169e1;
	border-radius: 10rpx;
	color: #fff;
	font-size: 26rpx;
	line-height: 60rpx;
	text-align: center;
}
.operate .btn {
	display: inline-block;
	width: 120rpx;
	height: 60rpx;
	box-sizing: border-box;
	border: 1px solid #4169e1;
	border-radius: 10rpx;
	background-color: #4169e1;
	color: #fff;
	font-size: 26rpx;
	line-height: 60rpx;
	text-align: center;
}

.btn-add {
	display: inline-block;
	width: 36rpx;
	height: 36rpx;
	background: transparent url(data:image/png;base64,) no-repeat;
	background-size: contain;
	margin-top: 36rpx;
}


image {
	vertical-align: top;
}